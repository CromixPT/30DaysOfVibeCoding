{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch MoodTracker (Server)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/MoodTracker/bin/Debug/net8.0/MoodTracker.dll",
      "args": [],
      "cwd": "${workspaceFolder}/MoodTracker",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "Now listening on: (https?://localhost:[0-9]+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "console": "integratedTerminal"
    }
  ]
}
